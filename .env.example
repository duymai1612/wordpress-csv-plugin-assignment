# WordPress CSV Plugin Environment Variables

# Database Configuration
DB_NAME=wordpress_csv_plugin
DB_USER=root
DB_PASSWORD=root
DB_HOST=localhost

# WordPress Configuration
WP_HOME=http://localhost:8080
WP_SITEURL=http://localhost:8080
WP_DEBUG=true
WP_DEBUG_LOG=true
WP_DEBUG_DISPLAY=false

# Plugin Configuration
CSV_PLUGIN_MAX_FILE_SIZE=10485760
CSV_PLUGIN_MAX_ROWS=10000
CSV_PLUGIN_BATCH_SIZE=100
CSV_PLUGIN_UPLOAD_DIR=wp-content/uploads/csv-imports

# Security
CSV_PLUGIN_JWT_SECRET=your-secret-key-here
CSV_PLUGIN_NONCE_SALT=your-nonce-salt-here

# Development
SCRIPT_DEBUG=true
SAVEQUERIES=true
